{
    "name": "xrelease/oauth2-xrel",
    "description": "xREL OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "keywords": [
        "oauth",
        "oauth2",
        "authorization",
        "authentication",
        "idp",
        "identity",
        "sso",
        "single sign on",
        "xrel",
        "xrel api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "xREL",
            "homepage": "https://www.xrel.to"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "league/oauth2-client": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "xREL\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "xREL\\OAuth2\\Client\\Test\\": "test/src/"
        }
    }
}
